{
  "name": "@golive/testing-framework",
  "version": "1.0.0",
  "description": "Advanced AI-powered testing framework for GoLive DevOps Platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:e2e\" \"npm run dev:load\"",
    "dev:api": "ts-node-dev --respawn src/api-testing/server.ts",
    "dev:e2e": "ts-node-dev --respawn src/e2e-testing/server.ts",
    "dev:load": "ts-node-dev --respawn src/load-testing/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:api": "npm run test -- --testPathPattern=api-testing",
    "test:e2e": "npm run test -- --testPathPattern=e2e-testing",
    "test:load": "npm run test -- --testPathPattern=load-testing",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "artillery": "^2.0.3",
    "axios": "^1.6.2",
    "cli-table3": "^0.6.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jest": "^29.7.0",
    "joi": "^17.11.0",
    "openai": "^4.20.1",
    "playwright": "^1.40.1",
    "selenium-webdriver": "^4.15.0",
    "supertest": "^6.3.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/selenium-webdriver": "^4.1.20",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "concurrently": "^8.2.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
